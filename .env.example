# ============================================
# AWS Credentials
# ============================================
# Get these from AWS IAM Console
# Required Permissions: S3 Full Access (or specific bucket permissions)
AWS_ACCESS_KEY_ID=YOUR_AWS_ACCESS_KEY
AWS_SECRET_ACCESS_KEY=YOUR_AWS_SECRET_KEY
AWS_REGION=us-east-1

# ============================================
# S3 Configuration
# ============================================
# S3 bucket name where CSV files will be stored
S3_BUCKET_NAME=your-bucket-name
AWS_S3_PREFIX=your-folder-prefix/

# ============================================
# IAM Configuration
# ============================================
AWS_IAM_ROLE_ARN=YOUR_IAM_ROLE_ARN

# ============================================
# Snowflake Credentials
# ============================================
# Account Identifier: Find this in your Snowflake URL
# Format: <account_locator>.<region>.<cloud> or <orgname>-<account_name>
SNOWFLAKE_ACCOUNT=YOUR_ACCOUNT_IDENTIFIER

# Snowflake Account Locator
SNOWFLAKE_ACCOUNT_LOCATOR=YOUR_ACCOUNT_LOCATOR

# Snowflake Username (usually your email or username)
SNOWFLAKE_USER=YOUR_USERNAME

# Snowflake Password
SNOWFLAKE_PASSWORD=YOUR_PASSWORD

# Snowflake Warehouse (default: COMPUTE_WH)
SNOWFLAKE_WAREHOUSE=COMPUTE_WH

# Snowflake Role (default: ACCOUNTADMIN)
SNOWFLAKE_ROLE=ACCOUNTADMIN

# Database name for this project
SNOWFLAKE_DATABASE=ECOMMERCE_DW

# Default schema (will create multiple layers)
SNOWFLAKE_SCHEMA=PUBLIC

# Snowflake Storage Integration IAM User ARN
STORAGE_AWS_IAM_USER_ARN=YOUR_IAM_USER_ARN

# Snowflake Storage Integration External ID
STORAGE_AWS_EXTERNAL_ID=YOUR_EXTERNAL_ID